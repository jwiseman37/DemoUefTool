{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "max-line-length": false,
    "variable-name": [true, "check-format", "allow-leading-underscore"],
    "indent": [true, "spaces", 2],
    "quotemark": [true, "single", "avoid-escape"]
  },
  "rulesDirectory": []
}
